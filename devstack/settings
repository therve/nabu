
NABU_DIR=$DEST/nabu
NABU_CONF_DIR=/etc/nabu
NABU_CONF=$NABU_CONF_DIR/nabu.conf
NABU_API_PASTE_FILE=$NABU_CONF_DIR/api-paste.ini

NABU_BIN_DIR=$(get_python_exec_prefix)

NABU_SERVICE_HOST=${NABU_SERVICE_HOST:-$SERVICE_HOST}
NABU_SERVICE_PORT=${NABU_SERVICE_PORT:-7345}
NABU_SERVICE_PROTOCOL=${NABU_SERVICE_PROTOCOL:-$SERVICE_PROTOCOL}

enable_service nabu-api

